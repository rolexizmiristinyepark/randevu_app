{
  "name": "rolex-randevu-sistemi",
  "version": "2.0.0",
  "description": "Rolex İzmir İstinyepark Randevu Yönetim Sistemi",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "build:analyze": "vite build --mode analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "size": "size-limit",
    "validate": "run-p type-check lint test:run size",
    "ci": "run-s type-check lint test:run build size"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@size-limit/preset-app": "^11.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-plugin-security": "^3.0.1",
    "gh-pages": "^6.3.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.56.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.2.0",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.0",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^4.0.8"
  },
  "keywords": [
    "rolex",
    "appointment",
    "booking",
    "calendar"
  ],
  "author": "Rolex İzmir İstinyepark",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
