# ============================================
# PRODUCTION ENVIRONMENT
# ============================================
# Copy this to .env.production and fill with real values

# Supabase Project URL (from Supabase Dashboard)
VITE_SUPABASE_URL=https://<project-ref>.supabase.co

# Supabase Anon Key (public, safe to expose in frontend)
VITE_SUPABASE_ANON_KEY=

# Production domain
VITE_BASE_URL=https://rolex-randevu.com

# Cloudflare Turnstile production site key
VITE_TURNSTILE_SITE_KEY=

# Production settings
VITE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_API_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_APP_VERSION=2.0.0
