# ============================================
# MIGRATION ENVIRONMENT
# ============================================
# Bu dosyayi .env.migration olarak kopyala ve doldur
# ASLA git'e commit etme!

# Mevcut Google Apps Script deployment URL
GAS_URL=https://script.google.com/macros/s/.../exec

# Supabase Project URL
SUPABASE_URL=https://<project-ref>.supabase.co

# Supabase Service Role Key (SECRET - asla frontend'e koyma!)
# Supabase Dashboard -> Settings -> API -> service_role
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Migrasyon sirasinda olusturulan staff kullanicilari icin gecici sifre
# Staff kullanicilari ilk giriste sifre degistirmeli
DEFAULT_PASSWORD=TempPass123!

# Buyuk tablolari atla (audit_log, message_log)
SKIP_AUDIT_LOG=true
SKIP_MESSAGE_LOG=true

# Dry run modu - veritabanina yazma, sadece kontrol et
DRY_RUN=false
