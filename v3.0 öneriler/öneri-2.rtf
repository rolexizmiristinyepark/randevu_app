function getCustomerEmailTemplate(data) \{\
    const \{ GREETING, CONFIRMATION, LOOKING_FORWARD, SECTION_TITLE, COMMON_LABELS, CHANGE_INFO, CONTACT_INFO, CLOSING, COMMON \} = CONFIG.EMAIL_TEMPLATES;\
    const \{ appointmentType, customerName, formattedDate, time, serviceName, staffName, customerNote, staffPhone, staffEmail \} = data;\
\
    let typeSpecificInfo = '';\
    if (appointmentType === CONFIG.APPOINTMENT_TYPES.DELIVERY) \{\
        typeSpecificInfo = `<p style="line-height: 1.8;">$\{CONFIG.EMAIL_TEMPLATES.DELIVERY.INFO\}</p>`;\
    \} else if (appointmentType === CONFIG.APPOINTMENT_TYPES.SERVICE) \{\
        typeSpecificInfo = `<p style="line-height: 1.8;">$\{CONFIG.EMAIL_TEMPLATES.SERVICE.INFO\}</p>`;\
    \} else \{\
        typeSpecificInfo = `<p style="line-height: 1.8;">$\{CONFIG.EMAIL_TEMPLATES.MEETING.INFO\}</p>`;\
    \}\
\
    const tableData = \{\
        DATE: formattedDate,\
        TIME: time,\
        SUBJECT: serviceName,\
        CONTACT_PERSON: staffName,\
        STORE: CONFIG.COMPANY_NAME,\
        NOTES: customerNote || COMMON.NOT_SPECIFIED\
    \};\
\
    const tableRows = Object.keys(COMMON_LABELS).map(key => `\
        <tr>\
            <td style="padding: 8px 12px 8px 0; font-weight: bold; width: 35%; vertical-align: top;">$\{COMMON_LABELS[key]\}</td>\
            <td style="padding: 8px 0; vertical-align: top; word-wrap: break-word;">$\{tableData[key]\}</td>\
        </tr>\
    `).join('');\
\
    return `\
        <div style="font-family: Arial, sans-serif; max-width: 600px; margin: 0 auto; color: #333;">\
            <p>$\{GREETING\} $\{customerName\},</p>\
            <p>$\{CONFIRMATION\}<br>$\{LOOKING_FORWARD\}</p>\
            <div style="margin: 30px 0; padding: 20px; background: #f9f9f9; border-left: 3px solid #C9A55A;">\
                <h3 style="margin-top: 0; color: #1A1A2E; font-weight: normal; letter-spacing: 1px;">$\{SECTION_TITLE\}</h3>\
                <table style="width: 100%; border-collapse: collapse; table-layout: fixed;">$\{tableRows\}</table>\
            </div>\
            $\{typeSpecificInfo\}\
            <p style="line-height: 1.8;">$\{CHANGE_INFO\}<br>$\{CONTACT_INFO\}</p>\
            <p style="margin-top: 30px; line-height: 1.8;">\
