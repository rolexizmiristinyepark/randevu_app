    const startDateTime = new Date(`$\{date\}T$\{time\}:00`);\
    const endDateTime = new Date(startDateTime.getTime() + (duration * 60 * 1000));\
\
    const formattedDate = new Date(date).toLocaleDateString('tr-TR', \{ day: 'numeric', month: 'long', year: 'numeric' \});\
    const appointmentTypeName = CONFIG.ICS_TEMPLATES.CUSTOMER_TYPES[appointmentType] || appointmentType;\
    const summary = `\uc0\u304 zmir \u304 stinyepark Rolex - $\{staffName\} ($\{appointmentTypeName\})`;\
    \
